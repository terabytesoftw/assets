{
    "name": "yiisoft/assets",
    "type": "library",
    "description": "Asset bundles and asset manager",
    "keywords": [
        "yii",
        "library",
        "assets"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/assets/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/assets"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "psr/log": "^1.1",
        "yiisoft/aliases": "^1.1.0",
        "yiisoft/files": "^3.0@dev"
    },
    "require-dev": {
        "npm-asset/sass": "^1.29.0",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.3",
        "vimeo/psalm": "^4.1",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config": {
            "sort-packages": true
        },
        "config-plugin": {
            "params": "config/params.php",
            "web": "config/web.php"
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
